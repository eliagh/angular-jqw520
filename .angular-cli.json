{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "angular-jqwidgets"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/jqwidgets-scripts/jqwidgets/styles/jqx.base.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxcore.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxbuttons.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxscrollbar.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxdata.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxdate.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxscheduler.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxscheduler.api.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxdatetimeinput.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxmenu.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxcalendar.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxtooltip.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxwindow.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxcheckbox.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxlistbox.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxdropdownlist.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxnumberinput.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxradiobutton.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/jqxinput.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/globalization/globalize.js",
        "../node_modules/jqwidgets-scripts/jqwidgets/globalization/globalize.culture.de-DE.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
